# Google OAuth Client ID
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Google Apps Script URL (for logging)
# 1. Create a Google Sheet
# 2. Go to Extensions > Apps Script
# 3. Paste the Apps Script code
# 4. Deploy as web app
# 5. Copy the URL here
VITE_GOOGLE_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Tracking Sheet URL (for mission tracking logs)
VITE_TRACKING_SCRIPT_URL=https://script.google.com/macros/s/YOUR_TRACKING_SCRIPT_ID/exec

# Interaction Log Sheet URL (for mouse/touch/scroll tracking)
VITE_INTERACTION_LOG_URL=https://script.google.com/macros/s/YOUR_INTERACTION_SCRIPT_ID/exec
